{
  "kind": "collectionType",
  "collectionName": "cm_pages",
  "info": {
    "singularName": "cm-page",
    "pluralName": "cm-pages",
    "displayName": "Cm-page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "url": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "unique": true
    },
    "meta_title": {
      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "h1": {
      "type": "string"
    },
    "description_h1": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "cm_pages"
    },
    "content": {
      "type": "richtext"
    },
    "pagesSimulaires": {
      "type": "component",
      "component": "cm-components.courses",
      "repeatable": true
    },
    "About": {
      "type": "component",
      "component": "cm-components.about",
      "repeatable": false
    },
    "bulltetPoint": {
      "type": "component",
      "component": "shared.bullet-point",
      "repeatable": false
    }
  }
}
